{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "import/newline-after-import": 0,
    "no-unused-vars": 0,
    "no-unused-expressions":0,
    // "no-shadow": 0,
    // "object-curly-newline": 0,
    "tfunc-names":0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "use strict": 0,
    "no-undef": 0,
    "no-param-reassign": 0,
    "camelcase": 0,
    // "prefer-arrow-callback": 0,
    // "comma-dangle": 0,
    // "func-names": ["error", "never"],
    "semi": 0,
    "Cannot redeclare block-scoped variable 'checkboxes'":0,
    "quotes": 0,
    "consistent-return": 0,
    // "max-len": ["error", { "code": 250 }],
    "no-await-in-loop": 0,
    "no-plusplus": 0,
		"no-console": "off",
    // "strict": ["error", "global"],
    // "curly": "warn",
    "react/jsx-props-no-spreading": "off",
    "jsx-a11y/label-has-associated-control":"off",
    "react/react-in-jsx-scope": "off",
    "max-len":["error", { "code": 900 }],
    "import/no-extraneous-dependencies": ["error",  {"devDependencies": true}],
    "react/button-has-type":"off",
    "react/no-unknown-property":"off",
    "react/jsx-filename-extension":"off",
    "react/jsx-no-constructed-context-values":"off",
    "jsx-a11y/no-static-element-interactions":"off",
    "no-nested-ternary":"off",
    "jsx-a11y/click-events-have-key-events":"off",
    "import/no-unresolved":"off",
    "import/prefer-default-export":"off",
    "default-param-last":"off",
    "react/self-closing-comp":"off",
    "import/order":"off",
    "react/style-prop-object":0,
    "jsx-a11y/alt-text":0,
    "jsx-quotes":0,
    "JSX attributes must only be assigned a non-empty 'expression'":0,
    "react/jsx-one-expression-per-line":0, //возможно стоит закоментить это сво-во
    "arrow-parens":0,
    "react/jsx-no-bind":0,
    "prefer-arrow-callback":0,
    "func-names":0,
    "react/prop-types":"off",
    "jsx-a11y/no-noninteractive-element-interactions":0,
    "no-prototype-builtins":0,
    "react/jsx-no-useless-fragment":0,
    "react/no-array-index-key":0,
    "no-restricted-globals":0,
    "no-alert":0,
    "no-restricted-syntax":0,
    "no-mixed-operators":0,
    "import/no-named-as-default":0,
    "import/no-named-as-default-member":0,
    "no-shadow":0
    }
}
